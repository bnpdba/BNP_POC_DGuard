# Oracle Settings
export TMP=/tmp
export TMPDIR=$TMP

export ORACLE_BASE={{ vm_OBASE }}
export ORACLE_HOME={{ vm_OHOME_CLI }}

export LD_LIBRARY_PATH=$ORACLE_HOME:/lib:/usr/lib:$LD_LIBRARY_PATH
export TNS_ADMIN=$ORACLE_HOME/network/admin

export ANSIBLE_CONFIG=/home/ansible/LCM/ANSIBLE_CFG/ansible.cfg
export ANSIBLE_TOP10=/home/ansible/LCM/ORAPROV_ANSIBLE/roles/10-Install_Uninstall

export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:$PATH:$ORACLE_HOME:$LD_LIBRARY_PATH
